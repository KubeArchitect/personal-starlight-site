{
  "ci": {
    "collect": {
      "startServerCommand": "npm run dev",
      "startServerReadyPattern": "Serving",
      "startServerReadyTimeout": "10000",

      "staticDistDir": "${process.env.GITHUB_WORKSPACE}/dist/",

      "numberOfRuns": 1,

      "settings": {
        "skipAudits": [
          "robots-txt",
          "canonical",
          "tap-targets",
          "is-crawlable",
          "works-offline",
          "offline-start-url"
        ]
      }
    }
  }
}