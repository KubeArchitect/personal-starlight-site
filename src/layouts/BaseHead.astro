---
import SpeedInsights from '@vercel/speed-insights/astro';
const { title, description } = Astro.props;
---
<html lang="en">
    <head>
        <title>{title}</title>
        <meta name="title" content={title} />
        <meta name="description" content={description} />
    </head>
    <body>
        <SpeedInsights />
    </body>
</html>