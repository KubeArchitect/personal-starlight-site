---
import SpeedInsights from '@vercel/speed-insights/astro';
const { siteTitle, description } = Astro.props;
---
<html lang="en">
    <head>
        <title>{siteTitle}</title>
        <meta name="title" content={siteTitle} />
        <meta name="description" content={description} />
    </head>
    <body>
        <SpeedInsights />
    </body>
</html>